<div class="box box-info">
	<div class="container">
		<div clas="box-header">
			<h1 class="box-title", style="padding-top: 10px">Edicion de Cuenta</h1>
		</div>
	</div>
	<div class="box-body">
		<%= render 'form', cuenta: @cuenta %>
			
				<div class="form-group">
					<div class="col-md-2">
					<%= link_to 'Mostrar', @cuenta , :class => "btn btn-block btn-info" %> 
					</div>
					<div class="col-md-2">
					<%= link_to 'Atras', cuentas_path, :class => "btn btn-block btn-default" %>
					</div>
				</div>
			
	</div>
	
</div>
