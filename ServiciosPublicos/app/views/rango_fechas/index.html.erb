<p id="notice"><%= notice %></p>

<h1>Rango Fechas</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rango_fechas.each do |rango_fecha| %>
      <tr>
        <td><%= rango_fecha.descripcion %></td>
        <td><%= link_to 'Show', rango_fecha %></td>
        <td><%= link_to 'Edit', edit_rango_fecha_path(rango_fecha) %></td>
        <td><%= link_to 'Destroy', rango_fecha, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rango Fecha', new_rango_fecha_path %>
