<%= simple_form_for(@cheque) do |f| %>
  <%= f.error_notification %>
<div class="box-body">
  <div class="form-inputs">

    <%= f.input :titular %>

    <%= f.input :banco %>

    <%= f.input :numero_cheque %>

    <%= f.input :fecha_disponibilidad %>

  </div>
<br>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
</div>
<% end %>
