<%= simple_form_for(@tarjeta) do |f| %>
  <%= f.error_notification %>
<div class="box-body">
  <div class="form-inputs">

    <%= f.input :nro_verificacion %>

    <%= f.input :titular %>

    <%= f.input :categoria_tarjeta %>

    <%= f.input :tipo_tarjeta %>

  </div>
<br>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
</div>
<% end %>
