<%= simple_form_for(@servicio) do |f| %>
  <%= f.error_notification %>
    <div class="box-body">
      <div class="form-inputs">
        <%= f.input :consumo_minimo %>

        <%= f.input :costo_unitario %>

        <%= f.input :unidad_medida %>

        <%= f.input :descripcion %>
      </div>
    </div>

    <div class="box-footer" align="right" >
  <div class="form-actions">
        <a href="/servicios"><button type="button" class="btn btn-default">Regresar</button></a>
        <input class="btn  btn-success" type="submit" value="Guardar">
      </div>
    </div>
    
<% end %>
