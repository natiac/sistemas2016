<%= simple_form_for(@asiento) do |f| %>
  <%= f.error_notification %>
<div class="box-body">
  <div class="form-inputs">

    <%= f.input :numero ,  :autofocus=> true%>

    <%= f.input :fecha %>

    <%= f.input :debe %>

    <%= f.input :haber %>

  </div>

  <div class="form-actions">
    <%= f.submit  "Crear" , class: "btn btn-block  btn-success "  , style:"color:#fff" %>
  </div>
  </div>
<% end %>
