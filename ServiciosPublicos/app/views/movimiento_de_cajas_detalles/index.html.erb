<p id="notice"><%= notice %></p>

<h1>Movimiento De Cajas Detalles</h1>

<table>
  <thead>
    <tr>
      <th>Monto parcial</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @movimiento_de_cajas_detalles.each do |movimiento_de_cajas_detalle| %>
      <tr>
        <td><%= movimiento_de_cajas_detalle.monto_parcial %></td>
        <td><%= link_to 'Show', movimiento_de_cajas_detalle %></td>
        <td><%= link_to 'Edit', edit_movimiento_de_cajas_detalle_path(movimiento_de_cajas_detalle) %></td>
        <td><%= link_to 'Destroy', movimiento_de_cajas_detalle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

