<%= simple_form_for(@arqueo_caja) do |f| %>
  <%= f.error_notification %>
<div class="box-body">
  <div class="form-inputs">

    <%= f.input :fecha %>

    <%= f.input :monto_en_caja %>

    <%= f.input :monto_real %>

    <%= f.input :diferencia %>

  </div>
<br>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
</div>
<% end %>
